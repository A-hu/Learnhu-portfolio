<div class="form-group nested-fields">
  <%= f.text_field :title, class: 'form-control', placeholder: 'title' %>
  <%= f.number_field :percent_utilized, in: 1..100, class: 'form-control', placeholder: '1 to 100' %>
  <%= link_to_remove_association 'Remove Skill', f, class: 'btn btn-warning' %>
</div>
