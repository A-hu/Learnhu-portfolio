<% if current_user.class == User %>
  <% if current_user.like_blogs.include?(@blog) %>
    <%= link_to fa_icon('thumbs-up'), like_blog_path(@blog), method: :post, remote: true %>
  <% else %>
    <%= link_to fa_icon('thumbs-o-up'), like_blog_path(@blog), method: :post, remote: true %>
  <% end %>
<% end %>
